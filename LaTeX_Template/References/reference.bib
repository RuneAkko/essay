@TechReport{twcc,
    number =    {draft-holmer-rmcat-transport-wide-cc-extensions-01},
    type =      {Internet-Draft},
    institution =   {Internet Engineering Task Force},
    publisher = {Internet Engineering Task Force},
    note =      {Work in Progress},
    url =       {https://datatracker.ietf.org/doc/html/draft-holmer-rmcat-transport-wide-cc-extensions-01},
        author =    {Stefan Holmer and Magnus Flodman and Erik Sprang},
    title =     {{RTP Extensions for Transport-wide Congestion Control}},
    pagetotal = 11,
    year =      2015,
    month =     oct,
    day =       19,
    abstract =  {This document proposes an RTP header extension and an RTCP message for use in congestion control algorithms for RTP-based media flows. It adds transport-wide packet sequence numbers and corresponding feedback message so that congestion control can be performed on a transport level at the send-side, while keeping the receiver dumb.},
}

